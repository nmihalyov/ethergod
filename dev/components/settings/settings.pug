mixin settings
  section.settings.container
    .wrapper
      p.settings__heading Настройки
      .settings__head
        p.settings__head-title Настройки
        .settings__head-filter.js-settings-filter
          a.settings__head-filter-item.js-settings-filter-item(href="settings.html") Общие
          .settings__head-filter-inner.js-settings-filter-inner
            a.settings__head-filter-item.js-settings-filter-item(href="security.html") Безопасность
            a.settings__head-filter-item.js-settings-filter-item(href="preferences.html") Предпочтения
            a.settings__head-filter-item.js-settings-filter-item(href="blacklist.html") Чёрный список
            a.settings__head-filter-item.js-settings-filter-item(href="seessions.html") Сессии
            a.settings__head-filter-item.js-settings-filter-item(href="verification.html") Верификация
      .settings__grid
        aside.settings__sidebar
          .settings__filter
            a.settings__link.settings__link--active(href="settings.html") Общие
            a.settings__link(href="security.html") Безопасность
            a.settings__link(href="preferences.html") Предпочтения
            a.settings__link(href="blacklist.html") Чёрный список
            a.settings__link(href="sessions.html") Сессии
            a.settings__link(href="verification.html") Верификация
        main.settings__main
          .settings__col
            form.settings__card(action="")
              +input('email', 'Электронная почта', 'email', 'Введите e-mail', 'tellmeimamazing@gmail.com', 'settings__input')
              +button-primary('submit', 'Отправить подтверждение')
            form.settings__card(action="")
              +input('text', 'Telegram', 'telegram', 'Имя пользователя Telegram', 'tellmeimamazing@gmail.com', 'settings__input')
              +input('text', 'Skype', 'skype', 'Skype ID', 'tellmeimamazing@gmail.com', 'settings__input')
              +button-primary('submit', 'Сохранить изменения')
          .settings__col
            form.settings__card(action="")
              .settings__phone
                .settings__phone-col
                  +input('tel', 'Номер телефона', 'phone', '+7 (___) ___-__-__', '', 'settings__phone-input')
